/usr/sbin/dseditgroup -o create -q -r "No Group" -i -1 -g abcdefab-cdef-abcd-efab-cdefffffffff -S S-1-5-21-600 nogroup
/usr/sbin/dseditgroup -o create -q -r "System Group" -i 0 -g abcdefab-cdef-abcd-efab-cdef00000000 -S S-1-5-21-100 wheel
