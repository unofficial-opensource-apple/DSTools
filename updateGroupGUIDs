#!/bin/sh
/usr/sbin/dseditgroup -o create -q -r "Everyone" -c "Group membership calculated by system" -i 12 -g abcdefab-cdef-abcd-efab-cdef0000000C -S S-1-1-0 everyone
/usr/sbin/dseditgroup -o create -q -r "Authenticated Users" -c "Group membership calculated by system" -i 50 -g abcdefab-cdef-abcd-efab-cdef00000032 -S S-1-5-11 authedusers
/usr/sbin/dseditgroup -o create -q -r "Interactive" -c "Group membership calculated by system" -i 51 -g abcdefab-cdef-abcd-efab-cdef00000033 -S S-1-5-4 interactusers
/usr/sbin/dseditgroup -o create -q -r "Network" -c "Group membership calculated by system" -i 52 -g abcdefab-cdef-abcd-efab-cdef00000034 -S S-1-5-2 netusers
